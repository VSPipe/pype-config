{
    "options": {
        "opacity": 1,
        "x_offset": 5,
        "y_offset": 5,
        "bg_padding": 5,
        "bg_opacity": 0.5,
        "font_size": 42
    },
    "__available_keys__": [
        "{slate_frame_start}",
        "{slate_frame_end}",
        "{anatomy[publish][burninVersion]}"
    ],
    "burnins":{
        "TOP_LEFT": {
            "function": "datetime",
            "format": "%d.%m.%Y"
        },
        "TOP_RIGHT": {
            "function": "text",
            "text": "{anatomy[publish][burninVersion]}"
        },
        "BOTTOM_RIGHT": {
            "function": "frame_numbers",
            "text": "{frame_start}-{current_frame}-{frame_end}"
        },
        "BOTTOM_CENTERED": {
            "function": "text",
            "text": "{asset}"
        },
        "BOTTOM_LEFT": {
            "function": "text",
            "text": "{username}"
        }
    }
}
