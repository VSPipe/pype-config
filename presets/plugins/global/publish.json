{
    "IntegrateMasterVersion": {
        "enabled": false
    },
    "ExtractReview": {
        "__documentation__": "http://pype.club/docs/admin_presets_plugins",
        "profiles": [{
            "families": [],
            "tasks": ["compositing"],
            "hosts": ["maya", "nuke"],
            "outputs": {
                "h264": {
                    "output_filter": {
                        "families": ["render", "ftrack"],
                        "tags": []
                    },
                    "width": 1920,
                    "height": 1080,
                    "aspect_ratio": 1,
                    "ext": ".mov",
                    "ffmpeg_args": {
                        "input": [
                            "-gamma 2.2"
                        ],
                        "video_filters": [],
                        "audio_filters": [],
                        "output": [
                            "-pix_fmt yuv420p",
                            "-crf 18",
                            "-intra"
                        ]
                    },
                    "tags": ["burnin", "preview"]
                }
            }
        }]
    }
}
