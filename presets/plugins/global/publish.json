{
    "ExtractReview": {
        "ext_filter": ["exr", "jpg", "jpeg", "mov", "png", "dpx", "mp4"],
        "outputs": {
            "h264": {
                "families": ["write", "ftrack"],
                "input": [
                    "-gamma 2.2"
                ],
                "output": [
                    "-pix_fmt yuv420p",
                    "-crf 18"
                ],
                "output_backup": [
                    "-c:v libx264",
                    "-vf colormatrix=bt601:bt709"
                ],
                "tags": ["preview", "burnin"]
            },
            "mxf": {
                "families": ["write", "ftrack"],
                "input": [
                    "-r 25"
                ],
                "output": [
                    "-vcodec dnxhd",
                    "-pix_fmt yuv422p",
                    "-b:v 185M",
                    "-ar 48000",
                    "-ac 2",
                    "-qmax 51"
                ]
            }
        }
    }

}
