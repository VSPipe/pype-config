{
    "ExtractReview": {
        "documentation": {
            "add letter_box": {
                "letter_box": 2.35
            },
            "rescale up to 1920x1080": {
                "reformat": true
            }
        },
        "ext_filter": ["exr", "jpg", "jpeg", "mov", "png", "dpx", "mp4"],
        "outputs": {
            "h264": {
                "families": ["write", "ftrack"],
                "input": [
                    "-gamma 2.2"
                ],
                "reformat": true,
                "output": [
                    "-pix_fmt yuv420p",
                    "-crf 18"
                ],
                "tags": ["preview", "burnin"],
                "ext": "mov"
            },
            "h264_keep": {
                "families": ["write"],
                "input": [
                    "-gamma 2.2"
                ],
                "reformat": true,
                "output": [
                    "-pix_fmt yuv420p",
                    "-crf 18"
                ],
                "tags": ["burnin"],
                "ext": "mov"
            },
            "edit": {
                "families": ["write"],
                "output": [
                    "-vcodec dnxhd",
                    "-profile:v dnxhr_hq",
                    "-pix_fmt yuv422p",
                    "-b:v 120M",
                    "-ar 48000",
                    "-ac 2",
                    "-qmax 51"
                ],
                "ext": "mxf"
            },
            "wipmov": {
                "families": ["write"],
                "output": [
                    "-c:v prores_ks",
                    "-profile:v 3"
                ],
                "tags": ["burnin"],
                "ext": "mov"
            }
        }
    }
}
